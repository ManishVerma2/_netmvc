@model List<Employee>
<h1>Welcome to the Employee Data</h1>

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Department</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Department</td>
        </tr>
    }
</table>
